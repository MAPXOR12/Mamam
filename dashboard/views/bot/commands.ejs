<%- include ("../parts/head",{bot,req})%>
  
  
    
    <style>
    
    .command-list {
      display:grid;
      gap:10px;
      background:var(--nav-color);
justify-content: space-between;
    }
    .command-list li{
      
      border:3px solid var(--nav-color);
    
      color:var(--white);
      
      list-style:none;
    }
    .command-list li details p{
 background:var(--navlist-color);
      width:250px;
    }
  </style>
  <div class="container">
  <h3 class="btn btn-danger" disabled> Some Of commands Transferred to dashboard</h3>
  <br>
  <br>
  <h3 class="btn btn-danger">This is not all commands</h3>
  
<table class="table table-dark table-striped">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th  scope="col">description</th>
          
        </tr>
    </thead>
        <tbody>
          <% bot.slash.map((a)=>{%>
          <tr>
        
     <td><%=a.name%></td>
            <td><%=a.description%></td>
            
          
          </tr>
              <% })%>
      </tbody>
      
            </table>
    </div>
  </div>
        
     
</div>

