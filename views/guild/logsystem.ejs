}
.slider{
  
position: absolute;
  cursor:pointer;
  left:0;
  right:0;
  transition:.4s;
  border-radius:40px;
  top:0;
  bottom:0;
  background:rgba(79, 78, 77, 0.74);;
}
  .switch #switch-form {
    display:none;
  }
.slider:before{
  position: absolute;
  content:"";
  height:28px;
  width:28px;
  box-shadow:0 0 10px 0 black;
  left:0;
  bottom:1px;
  background-color:white;
  transition:.4s;
  border-radius:50px;
  

}
  #switch-form:checked + .slider{

background:var(--switch-form-bgcolor);

  }
#switch-form:checked + .slider:before{
  
