          messageDeletechannel:$("#messageDeletechannel").val(),
          messageUpdatecolor:$(".messageUpdatecolor").val(), 
          messageUpdatechannel:$("#messageUpdatechannel").val(),
kickcolor:$(".kickcolor").val(), 
          kickchannel:$("#kickchannel").val()


          
          
            
    }).done(function(data){
                
                
                
                
                if(data.error){
                  const Toast = Swal.mixin({
  toast: true,
  position: 'top-end',
  showConfirmButton: false,
  timer: 3000,
  timerProgressBar: true,
  didOpen: (toast) => {
    toast.addEventListener('mouseenter', Swal.stopTimer)
    toast.addEventListener('mouseleave', Swal.resumeTimer)
  }
})

